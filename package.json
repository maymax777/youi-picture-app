{
  "name": "PictureApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "@youi/babel-plugin-react-native-youi": "1.0.4",
    "@youi/react-native-youi": "6.1.0",
    "react": "16.8.6",
    "react-native": "0.60.6",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "husky": ">=4",
    "jest": "^24.9.0",
    "lint-staged": ">=10",
    "metro-react-native-babel-preset": "0.57.0",
    "prettier": "^2.2.0",
    "react-native-dotenv": "^2.4.2",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "@youi/react-native-youi",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(jest-)?react-native|@youi)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/appium/"
    ]
  },
  "youi": {
    "entryFile": "index.youi.js",
    "defaultBundleMode": "remote"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
