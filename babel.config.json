{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    ["module:@youi/babel-plugin-react-native-youi"],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@containers": "./src/containers",
          "@redux": "./src/redux",
          "@services": "./src/services",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
